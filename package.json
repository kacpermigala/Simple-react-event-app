{
  "name": "eventapp",
  "version": "0.1.0",
  "private": true,
  "author": "Kacper Migala",
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/core": "^4.4.2",
    "@material-ui/pickers": "^3.2.6",
    "classnames": "^2.2.6",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^2.2.1",
    "es6-promise": "^4.2.8",
    "es6-shim": "^0.35.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.15",
    "normalize.css": "^8.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "urijs": "^1.19.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eslint": "eslint --max-warnings 0 src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-prettier": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "^4.12.0"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/index.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "<rootDir>/src/tests/transform.js"
    },
    "testMatch": [
      "**/tests/**/*-test.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/src/tests/mocks/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
